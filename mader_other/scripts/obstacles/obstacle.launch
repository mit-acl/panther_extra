<launch>
	<arg name="quad" default="SQ01s"/>

    <node ns="$(arg quad)" name="obstacle" pkg="mader_other" type="obstacle.py" output="screen" clear_params="true">
        <rosparam file="$(find mader_other)/scripts/obstacles/$(arg quad).yaml" command="load"/>
    </node>

    <node ns="$(arg quad)" name="commands" pkg="mader_other" type="commands.py" output="screen"/>



</launch>
